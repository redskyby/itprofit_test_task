{
    "compilerOptions": {
        "outDir": "./dist/",
        "noImplicitAny": true,
      "module": "ESNext",
      "target": "es5",
        "jsx": "react",
        "allowJs": true,
        "moduleResolution": "node",
      "allowSyntheticDefaultImports": true,
      "esModuleInterop": true
    },
  "ts-node": {
    "compilerOptions": {
      "module": "CommonJS"
    }
  },
  "exclude": [
    "node_modules"
  ]
}